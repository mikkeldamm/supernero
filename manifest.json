{
    "name": "Supernero",
    "description": "Supernero will enhance Dinero to be usefull for people not working in Dinero.",
    "version": "1.7",
    "manifest_version": 3,
    "content_scripts": [
        {
            "css": ["content.css"],
            "js": ["content.js"],
            "matches": ["https://app.dinero.dk/*"]
        }
    ],
    "action": {
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png",
            "256": "/images/icon_256.png",
            "512": "/images/icon_512.png",
            "1024": "/images/icon_1024.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png",
        "256": "/images/icon_256.png",
        "512": "/images/icon_512.png",
        "1024": "/images/icon_1024.png"
    }
}
